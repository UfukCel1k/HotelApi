@model CreateServiceDto
@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}


<form method="post">
    <label>Servis İkon</label>
    <input type="text" asp-for="ServiceIcon" class="form-control" />
    <span asp-validation-for="ServiceIcon" class="text-danger"></span>
    <br />
    <label>Personel Ünvanı</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Sosyal Medya 1</label>
    <input type="text" asp-for="SocialMedia1" class="form-control" />
    <br />
    <label>Sosyal Medya 2</label>
    <input type="text" asp-for="SocialMedia2" class="form-control" />
    <br />
    <button class="btn mb-1 btn-rounded btn-outline-primary">Personel Ekle</button>
</form>

